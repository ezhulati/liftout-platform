{
  "name": "@liftout/api-server",
  "version": "1.0.0",
  "private": true,
  "description": "Liftout API Server",
  "main": "dist/index.js",
  "scripts": {
    "dev": "lsof -ti:8000 | xargs kill -9 2>/dev/null || true; tsx watch src/index.ts",
    "build": "prisma generate && tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^5.5.0",
    "@sendgrid/mail": "^7.7.0",
    "@socket.io/redis-adapter": "^8.3.0",
    "aws-sdk": "^2.1480.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.12.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.0",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^7.1.0",
    "express-validator": "^7.0.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "1.4.5-lts.2",
    "multer-s3": "^3.0.1",
    "nodemailer": "^6.9.0",
    "resend": "^6.5.2",
    "socket.io": "^4.7.0",
    "stripe": "^14.0.0",
    "winston": "^3.11.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.4",
    "@types/compression": "^1.7.4",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.5",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/morgan": "^1.9.6",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.8.0",
    "@types/nodemailer": "^6.4.11",
    "@types/supertest": "^2.0.15",
    "jest": "^29.7.0",
    "prisma": "^5.5.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "tsx": "^3.14.0",
    "typescript": "^5.2.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}